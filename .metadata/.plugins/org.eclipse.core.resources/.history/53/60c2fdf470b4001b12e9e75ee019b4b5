package study;

import java.util.Scanner;

public class Exam05 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		System.out.println("별( * )을 출력할 행 수를 입력하시오 >> ");
		int num = sc.nextInt();
		if(int i = 1; i<=num ; i++)
		{
			if(int j =1; j<=num; j++) {
				System.out.print("*");
			}
			System.out.println();
		}
		
	}

}
