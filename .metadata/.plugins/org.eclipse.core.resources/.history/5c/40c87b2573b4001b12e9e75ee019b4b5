package study;

import java.util.Scanner;

public class Exam06 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		System.out.print("별(*)의 행수를 입력해주세요>>");
		int num = sc.nextInt();
		
		for(int i=1; i<=num; i++)
		{
			for(int j=1; j<=num-i; j++)
			{
				System.out.println(" ");
			}
			for(int i=1; i<=num; i++) {
			System.out.print("*");
			}
			System.out.println();
		}
	}

}
